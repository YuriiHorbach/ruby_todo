<p id="notice"><%= notice %></p>

<h1>Todo Lists</h1>

<div class="container">
  <div class="row">
    <div class="col-lg-5">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th scope="col">Title</th>
          </tr>
        </thead>
        <tbody>
          <% @todo_lists.each do |todo_list| %>
              <tr>
                <td><%= todo_list.title %></td>
                <td><%= link_to 'Show', todo_list %></td>
                <td><%= link_to 'Edit', edit_todo_list_path(todo_list) %></td>
                <td><%= link_to 'Destroy', todo_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
          <% end %>
          <%= render(@todo_lists) || content_tag("p", "There are no todo lists.") %>
        </tbody>
      </table>
    </div>
  </div>
</div>


